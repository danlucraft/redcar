require ENV['TM_SUPPORT_PATH'] + '/lib/textmate.rb'
require ENV['TM_SUPPORT_PATH'] + '/lib/ui.rb'

USR_HEAD  = Regexp.escape "#include \"...\" search starts here:\n"
SYS_HEAD  = Regexp.escape "#include <...> search starts here:\n"
FOOTER    = Regexp.escape 'End of search list.'

COMPILER  = {
  'source.c'      => 'gcc 2>&1 >/dev/null -E -v -x c /dev/null',
  'source.c++'    => 'g++ 2>&1 >/dev/null -E -v -x c++ /dev/null',
  'source.objc'   => 'gcc 2>&1 >/dev/null -E -v -x objective-c /dev/null',
  'source.objc++' => 'g++ 2>&1 >/dev/null -E -v -x objective-c++ /dev/null',
}

def compiler_search_path(domain = :system)
  scope        = 'source.c'
  scope        = $& if ENV['TM_SCOPE'] =~ /source.(obj)?c(\+\+)?/

  res = IO.popen(COMPILER[scope]) { |io| io.read }
  if res =~ /#{USR_HEAD}(.*)#{SYS_HEAD}(.*)#{FOOTER}/m
    case domain
      when :system  then $2
      when :user    then $1 + " .\n"
      when :all     then $1 + " .\n" + $2
    end.scan(/ (.*)/).flatten
  else
    abort "Failed to parse compiler output.\nCommand: " + COMPILER[scope]
  end
end

def user_search_path(domain = :system)
  usr = ENV['TM_USR_HEADER_PATH'].to_s
  sys = ENV['TM_SYS_HEADER_PATH'].to_s

  res = case domain
    when :system  then sys
    when :user    then usr
    when :all     then "#{usr}:#{sys}"
  end.split(':')
  res.delete('')

  res.empty? ? nil : res
end

def header
  if ENV.has_key? 'TM_SELECTED_TEXT'
    [:all, ENV['TM_SELECTED_TEXT']]
  elsif ENV['TM_CURRENT_LINE'] =~ /#\s*(?:include|import)\s*([<"])(.*?)[">]/;
    [$1 == '<' ? :system : :user, $2]
  else
    header = TextMate::UI.request_string :title => "Quick Open", :prompt => "Which header file do you wish to open?"
    [:all, header]
  end
end

def find_in_dirs(file, dirs)
  framework = file.sub(/(.*)\/(.*\.h)/, '\1.framework/Headers/\2')
  base = ENV['TM_DIRECTORY'] || Dir.getwd
  dirs.each do |dir|
    dir = "#{base}/#{dir}" unless dir[0..0] == "/"
    return "#{dir}/#{file}"       if File.exists? "#{dir}/#{file}"
    return "#{dir}/#{framework}"  if File.exists? "#{dir}/#{framework}"
  end
  nil
end

def find_recursively(header)
  hdr_match = /\b#{Regexp.escape(header.sub(/(\.h)?$/, '.h'))}$/i
  matches   = Dir["/System/Library/Frameworks/*.framework/{Frameworks/*.framework/,}Headers/*.h"].find_all { |e| e =~ hdr_match }

  if matches.size > 1
    menu = matches.map do |e|
      header_name = $& if e =~ /[^\/]+$/
      framework_suffix = e.scan(/\/([^\/]+?).framework/).flatten.join(' ‚Üí ')
      { 'path' => e, 'title' => "#{header_name} ‚Äî #{framework_suffix}" }
    end
    if res = TextMate::UI.menu(menu)
      res['path']
    else
      nil
    end
  else
    matches.first
  end
end

domain, file = header()
abort if file.nil?

dirs = user_search_path(domain) || compiler_search_path(domain)
if path = find_in_dirs(file, dirs)
  TextMate.go_to :file => path
elsif domain == :all && path = find_recursively(file)
  TextMate.go_to :file => path
else
  abort "Unable to find ‚Äò#{file}‚Äô\nLocations searched:\n" + dirs.map { |dir| "   #{dir}" }.join("\n")
end
