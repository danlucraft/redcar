# Prepare output window.
html_header 'Typeset &amp; View' "$FILE"
echo '<h2>Compiling LaTeX‚Ä¶</h2>'

# Check if we actually have a tex compiler
require_cmd "$TEX" "You can set the <tt>PATH</tt> variable to include the path of <tt>$TM_LATEX_PROGRAM</tt>."

tex_command () {
if [ "$TEX" != latexmk.pl ]
  then echo "'$TEX' ${TM_LATEX_OPTIONS:=-interaction=nonstopmode -file-line-error-style}"
  else
    if [[ "$TM_LATEX_PROGRAM" == latex ]]
      then
        echo "'$TEX' -f -ps"
      else
        echo "'$TEX' -f -r '${TM_BUNDLE_SUPPORT}/latexmkrc'"
    fi
