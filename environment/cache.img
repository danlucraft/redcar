def esc(str)
  e_sn(str).gsub(/\}/, '\\}') # escaping inside a placeholder
end

s = STDIN.read
if s.empty? then
  print "*$1*"
elsif s =~ /^(\*+)(.*[^\*])\*+$/ then
  m, s = $1, $2
  case m.length
    when 1 then print "${1:#{esc s}}"
    when 2 then print "${1:***#{esc s}***}"
    else print "${1:**#{esc s}**}"
  end
